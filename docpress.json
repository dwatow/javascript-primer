{
  "name": "JavaScript Primer",
  "docs": "docs",
  "github": "dwatow/javascript-primer",
  "lang": "zh-tw",
  "css": [
    "docs/css/hackmd.css",
    "https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@9.16.2/build/highlight.min.js",
    "https://cdnjs.cloudflare.com/ajax/libs/KaTeX/0.5.1/katex.min.css"
  ],
  "scripts": [
    "js/prism/line-number.js"
  ],
  "markdown": {
    "plugins": [
      "abbr",
      {
        "name": "container",
        "options": "success"
      },
      {
        "name": "container",
        "options": "info"
      },
      {
        "name": "container",
        "options": "warning"
      },
      {
        "name": "container",
        "options": "danger"
      },
      {
        "name": "emoji",
        "options": {
          "shortcuts": {}
        }
      },
      {
        "name": "anchor",
        "options": {
          "level": 1,
          "permalink": false,
          "permalinkClass": "header-anchor",
          "permalinkSymbol": "Â¶",
          "permalinkBefore": false
        }
      },
      "deflist",
      "footnote",
      "imsize",
      "ins",
      "mark",
      "regexp",
      "sub",
      "sup",
      "checkbox",
      "katex",
      "video"
    ]
  }
}